// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int i, t1,t2,num,nextterm;
    printf("Enter a number\n");
    scanf("%d",&num);
    
    t1=0;
    t2=1;
    printf("first 2 terms %d %d \n",t1,t2);
    
    nextterm=t1+t2;
    for(i=3;i<=num;i++)
    {
        printf("%d\n", nextterm);
        t1=t2;
        t2=nextterm;
        nextterm=t1+t2;
    }

    return 0;
}
