// Online C compiler to run C program online
#include <stdio.h>
int constring(char* str1, char* str2)
{
    while(*str1)
    {
        *str1++;
    }
    while(*str2)
    {
       *str1++=*str2++;
        
    }
    *str1='\0';
}
int main() {
    char string1[100]="Geeks ";
    char string2[200]="for geeks";
    
    constring(string1, string2);
    printf("%s\n",string1);
    

    return 0;
}
