#include<stdio.h>
int main()
{
    int num;
    
    printf("Enter a number\n");
    scanf("%d", & num);
    
    if(num%5==0)
    {
    printf("divisible by 5");
    }
    
    else if(num%11==0)
    {
        printf("divisible by 11");
    }
    else
    {
        printf("not divisible by 5 and 11");
    }
}
