// Online C compiler to run C program online
#include <stdio.h>

int main() {
  int i,j,n,count;
  
  printf("Enter a number\n");
  scanf("%d",&n);
  printf("prime number from 1 to %d\n",n);
  
  for(i=1;i<=n;i++)
  {
  count=0;
  for(j=1;j<=i;j++)
  
  if(i%j==0)
  {
  count++;
  }
  
  if(count==2)
  {
      printf("%d\n",i);
  }
  }
  
  
  
    return 0;
}
