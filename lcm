// Online C compiler to run C program online
#include <stdio.h>

int main() {
   int n,max,a,b;
    printf("Enter two number\n");
    scanf("%d %d",&a,&b);
    max=(a>b) ? a:b;

  while(1)
  {
  if(max % a==0 && max % b==0)
  {
      printf("Lcm of %d and %d is %d",a,b,max);
      break;
  }
    max++;  
  }
  
    return 0;
}
