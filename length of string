// Online C compiler to run C program online
#include <stdio.h>
int stringlength(char *str)
{
    int length=0;
    while(*str!='\0')
    {
        length++;
        str++;
    }
    return length;
}
int main() {
    char str[1000]={"geeksforgeeks"};
    int length=stringlength(str);
    
    printf("length of string is ; %d\n",length);

    return 0;
}
