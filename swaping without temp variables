// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int x,y;
    printf("Enter x= ");
    scanf("%d",&x);
    printf("Enter y= ");
    scanf("%d",&y);
    
    printf("numbers before swap x= %d, y= %d",x,y);
    
   x=x+y;
   y=x-y;
   x=x-y;
   
    
    printf("\nAfter swap x= %d and y= %d",x,y);
    return 0;
}
