// Online C compiler to run C program online
#include <stdio.h>
#define max 50
int main() {
   int a[max][max],b[max][max],result[max][max];
   int arows,acolumn,brows,bcolumn;
   int i,j,k;
   int sum=0;
   
   printf("Enter matrix a is :");
   scanf("%d %d",&arows,&acolumn);
   
   printf("Enter the elements of matrix a is :");
   for(i=0;i<arows;i++)
   {
       for(j=0;j<acolumn;j++)
       {
           scanf("%d", &a[i] [j]);
       }
   }
   
   printf("Enter matrix b is :");
   scanf("%d %d",&brows,&bcolumn);
   
   printf("Enter the elements of matrix a is :");
   for(i=0;i<brows;i++)
   {
       for(j=0;j<bcolumn;j++)
       {
           scanf("%d",&b[i][j]);
       }
   }
  printf("\n");
  
  for(i=0;i<arows;i++)
  {
      for(j=0;j<bcolumn;j++)
      {
          for(k=0;k<brows;k++)
          {
              sum=sum+a[i][k]*b[k][j];
          }
          result[i][j]=sum;          
          sum=0;
      }
  }
  
  printf("Resultant matrix\n");
  for(i=0;i<arows;i++)
  {
      for(j=0;j<bcolumn;j++)
      {
          printf("%d ",result[i][j]);
      }
      printf("\n");
  }
  
    return 0;
}
