// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,i=2,flag=0;
    
    printf("Enter a number");
    scanf("%d",&n);
    
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
        printf("is not prime");
        flag=1;
        break;
        }
    }
    if(flag==0)
    printf("is prime number");
    return 0;
}
