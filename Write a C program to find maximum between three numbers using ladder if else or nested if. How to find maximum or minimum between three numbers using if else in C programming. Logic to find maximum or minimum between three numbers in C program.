// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,b,c;
    printf("Enter the number\n");
    scanf("%d %d %d", & a, & b, & c);
    
    if(a>b && a>c)
{
    printf("maximum is %d",a);
}
    else if(b>c && b>a)
{
    printf("maximum is %d",b);
}

if(c>a && c>b)
{
    printf("maximum is %d",c);
}

    return 0;
}
