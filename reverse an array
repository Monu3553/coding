// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int arr[5]={1,2,3,4,5};
    int i;
    printf("array is : ");
    for(i=0;i<5;i++)
    printf("%d",arr[i]);
    
    int rev[5];
    int j;
    
   
    for(i=5-1;i>=0;i--)
    {
        rev[j]=arr[i];
        j++;
    }
     printf("\nreverse array is : ");
    for(i=0;i<5;i++)
    printf("%d",rev[i]);

    return 0;
}
